{
    "gpu": {
        "use": true,
        "id": 0
    },
    "model": "GraphSage",
    "dataset": "K3Colorable",
    "out_dir": "out/K3Colorable_graph_classification/",
    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 25,
        "init_lr": 1e-4,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 10,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    "net_params": {
        "num_train_data": 7000,
        "L": 4,
        "hidden_dim": 90,
        "out_dim": 90,
        "residual": true,
        "readout": "mean",
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "batch_norm": true,
        "sage_aggregator": "maxpool",
        "pos_enc": false
    }
}