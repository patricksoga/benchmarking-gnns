{
    "gpu": {
        "use": true,
        "id": 0
    },
    "model": "SAGraphTransformer",
    "dataset": "CSL",
    "out_dir": "out/CSL_sparse_SA_BN/",
    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 5,
        "init_lr": 0.0005,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 10,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 24
    },
    "net_params": {
        "num_train_data": 200,
        "L": 6,
        "n_heads": 10,
        "hidden_dim": 80,
        "out_dim": 80,
        "residual": true,
        "readout": "mean",
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "layer_norm": false,
        "batch_norm": true,
        "self_loop": false,
        "wl_pos_enc": false,
        "full_graph": false,
        "lpe_layers": 1,
        "pos_enc_dim": 20,
        "lpe_n_heads": 1,
        "edge_feat": false,
        "pos_enc": false,
        "spectral_attn": true,
        "lpe_dim": 8
    }
}