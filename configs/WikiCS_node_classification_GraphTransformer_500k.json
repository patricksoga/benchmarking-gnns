{
  "gpu": {
    "use": true,
    "id": 0
  },
  "model": "GraphTransformer",
  "dataset": "WikiCS",
  "out_dir": "out/WikiCS_node_classification/",
  "params": {
    "seed": 41,
    "epochs": 1000,
    "init_lr": 1e-9,
    "lr_reduce_factor": 0.5,
    "lr_schedule_patience": 25,
    "min_lr": 1e-5,
    "weight_decay": 5e-4,
    "print_epoch_interval": 5,
    "max_time": 24
  },
  "net_params": {
    "L": 6,
    "n_heads": 8,
    "hidden_dim": 80,
    "out_dim": 80,
    "residual": true,
    "readout": "sum",
    "in_feat_dropout": 0.0,
    "dropout": 0.0,
    "batch_norm": true,
    "layer_norm": false,
    "gpu_id": 0,
    "full_graph": false,
    "matrix_type": "A",
    "pow_of_mat": 1,
    "self-loop": false,
    "pos_enc": false,
    "rand_pos_enc": false,
    "learned_pos_enc": false,
    "wl_pos_enc": false
  }
}