{
    "gpu": {
        "use": true,
        "id": 0
    },
    "model": "PseudoGraphormer",
    "dataset": "SBM_CLUSTER",
    "out_dir": "out/SBMs_sparse_NoPE_BN/",
    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 32,
        "init_lr": 0.0005,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 10,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 24
    },
    "net_params": {
        "L": 10,
        "n_heads": 8,
        "hidden_dim": 80,
        "out_dim": 80,
        "residual": true,
        "readout": "mean",
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "layer_norm": false,
        "batch_norm": true,
        "self_loop": false,
        "wl_pos_enc": false,
        "full_graph": false,
        "in_deg_centrality": 128,
        "out_deg_centrality": 128,
        "spd_len": 512
    }
}